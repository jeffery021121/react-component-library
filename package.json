{
  "name": "@jeffery021121/react-component-library",
  "version": "0.1.6",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc -p tsconfig.build.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook --no-dll --quiet",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coverall": "npm run coverage  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@jeffery021121"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffery021121/react-component-library.git"
  },
  "sideEffects": false,
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.stories.tsx",
      "src/**/*.tsx",
      "!src/**/*.snap"
    ]
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "polished": "^3.6.5"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "^3.7.5",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-a11y": "^6.0.10",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-docs": "^6.0.10",
    "@storybook/addon-knobs": "^6.0.10",
    "@storybook/addon-links": "^6.0.9",
    "@storybook/addon-storysource": "^6.0.10",
    "@storybook/addon-viewport": "^6.0.10",
    "@storybook/node-logger": "^6.0.9",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "react": ">=16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": ">=16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "^5.1.1",
    "typescript": "^3.7.5"
  }
}
